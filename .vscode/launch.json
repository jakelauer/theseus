{
	"version": "0.2.0",
	"configurations": [
	  {
		"type": "node",
		"request": "launch",
		"name": "Run Mocha Tests",
		"runtimeExecutable": "pnpm",
		"runtimeArgs": [
		  "test",
		  "--",
		  "--grep",
		  "\"should handle asynchronous mutations correctly\""
		],
		"internalConsoleOptions": "openOnSessionStart",
		"cwd": "${workspaceFolder}",
		"outputCapture": "std"
	  }
	]
  }
