{
	"private": false,
	"name": "theseus-js",
	"version": "1.0.0",
	"description": "Functionality to create and modify data fluently and narratively",
	"author": "Jake Lauer",
	"license": "MIT",
	"devDependencies": {
		"@testdeck/mocha": "^0.3.3",
		"@types/chalk": "2.2.0",
		"@types/prompts": "2.4.9",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"chalk": "4.1.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"prettier": "3.1.0",
		"prompts": "2.4.2",
		"ts-node": "^10.9.1",
		"typescript": "5.3.3",
		"arg": "^5.0.1"
	},
	"scripts": {
		"prepare": "cd ../.. && husky install",
		"build": "tsc --project ./tsconfig.json",
		"test": "mocha --exit --config ./.mocharc.js",
		"update": "pnpm build && node ./.scripts/update-version.js",
		"pub": "node ./.scripts/publish.js"
	},
	"lint-staged": {
		"*.ts": [
			"prettier --write",
			"eslint --fix --resolve-plugins-relative-to=./"
		]
	},
	"main": "dist/index.js",
	"directories": {
		"dist": "dist"
	},
	"engines": {
		"node": ">=16.x"
	}
}
